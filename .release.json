{
  "buildCommand": "npm run prepublish",
  "commitMessage": "release %s",
  "tagName": "v%s",
  "tagAnnotation": "release v%s",
  "non-interactive": true,
  "verbose": true,
  "dist": {
    "repo": "git@github.com:alexandersimoes/d3plus.git#gh-pages",
    "baseDir": "build",
    "files": ["*.js"],
    "pkgFiles": [],
    "afterReleaseCommand": "sudo rm -r .stage"
  },
  "github": {
    "release": true,
    "releaseName": "v%s"
  },
  "npm": {
    "publish": true,
    "forcePublishSourceRepo": true
  }
}
